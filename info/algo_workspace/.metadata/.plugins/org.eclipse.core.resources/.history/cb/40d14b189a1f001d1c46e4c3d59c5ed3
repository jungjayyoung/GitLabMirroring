package swea_ps;

import java.io.*;
import java.util.*;

public class p3234 {
	
	static boolean[] visited;
	static int[] choo;
	static int n;
	static List<Integer> rr;
	static List<Integer> ll;
	

	public static void main(String[] args) throws IOException{

		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		StringTokenizer st;
		
		int tt = Integer.parseInt(br.readLine());
		
		for(int t = 1; t <= tt; t++) {
			
			
			n = Integer.parseInt(br.readLine());
			visited = new boolean[n+1];
			
			st = new StringTokenizer(br.readLine()," ");
			for(int i = 0; i < n; i++) {
				choo[i] = Integer.parseInt(st.nextToken());			
			}
				
		}
		
	}
	
	static void subset(int cnt) {
		
		
		if(cnt == n) {
			int r_sum =0;
			int l_sum = 0;
			int r_cnt = 0;
			int l_cnt = 0;
			rr = new ArrayList<Integer>();
			ll = new ArrayList<Integer>();
			v = new boolean[10];
			
			for(int i =0; i <n; i++) {
				
				if(visited[i]) {
					r_sum += choo[i];
					rr.add(i);
				}else {
					l_sum += choo[i];
					ll.add(i);
				}
				
			}
			total = 0;
			perm(0,rr.size());
			r_cnt += total;
			total = 0;
			perm(0,ll.size());
			l_cnt += total;
			
			
			return;
		}
		
		visited[cnt] = true;
		subset(cnt + 1);
		visited[cnt] = false;
		subset(cnt + 1);
		
	}
	
	static boolean v[];
	static int total = 0;
	static void perm(int cnt, int size) {
		
		if(cnt == size) {
			
			total++;
			return;
		}
		
		for(int i = 0; i< size; i++) {
			
			if(!v[i]) {
				v[i] = true;
				perm(cnt + 1,size);
				v[i] = false;
			}
			
		}
		
		
	}

}
