public class p14891 {

	static int [][] a;
	
	static void turn(int []aa,int dir) {
		
		
		// 반시계 방향
		if(dir == -1) {
			
			int tmp = aa.get(0);
			aa.remove(0);
			aa.add(tmp);
			
		// 시계 방향
		}else if(dir == 1) {
			int tmp = aa.get(0);
			for(int i = 1; i < 8; i++) {
				tmp = aa.get(i);
				aa.set(i, element)
			}
		}
		
		
		
	}
	
	public static void main(String[] args) throws IOException{

		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		StringTokenizer st;
		
		a = new int[4][8];
	
		
		for(int i = 0; i <4; i++) {
			String s = br.readLine();
			
			// 12 시 부터 시계방향으로 주어진다.
			for(int j = 0; j <8;j++) {
				a[i][j] = s.charAt(j) - '0';
			}
		}
		
		
